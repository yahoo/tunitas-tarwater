# WATCHOUT - AM_LDFLAGS is not automatically added to the final link line anywhere (oddly enough...)
#            and also that LDFLAGS is reserved to the command line.
# if --with-nonstd-libhttpserver=yes (or similar) then we must link appropriately with -L, -Wl,-rpath=
bin_uss_LDFLAGS = $(PACKAGE_LDFLAGS_SET) -ltunitas-tarwater -lhttpserver -loptions-module
bin_uss_LDADD = lib/libwant.la 
bin_uss_MODULES = $(call TF_SOURCEStoMODULES, $(bin_uss_SOURCES))
bin_uss_SOURCES = \
  obj/src/tunitas/tarwater/uss/main.cpp \
  obj/src/tunitas/tarwater/uss/namespace.cpp \
  obj/src/tunitas/tarwater/uss/Configuration.cpp \
  obj/src/tunitas/tarwater/uss/options.indicator.cpp \
  obj/src/tunitas/tarwater/uss/options.indicator.constants.cpp \
  obj/src/tunitas/tarwater/uss/options.Specification.cpp \
  obj/src/tunitas/tarwater/uss/options.Parser.cpp \
  obj/src/tunitas/tarwater/uss/options.parser.Result.cpp \
  obj/src/tunitas/tarwater/uss/constants.cpp \
  obj/src/tunitas/tarwater/uss/endpoint.cpp \
  obj/src/tunitas/tarwater/uss/endpoint.Sync.cpp \
  obj/src/tunitas/tarwater/uss/exception.Quitting.cpp \
  obj/src/tunitas/tarwater/uss/Usage.cpp \
  $(end)
$(scold_cxxdir)/src/tunitas/tarwater/uss/dependencies.mk:
	$(AM_V_BB1ST) $(call TF_DISAGGREGATE_COMPILE, $(bin_uss_SOURCES))
	$(AM_V_BB2ND) $(call TF_DEPENDENCIES_COMPILE, $(bin_uss_SOURCES))
	test -e $@
include $(scold_cxxdir)/src/tunitas/tarwater/uss/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/src/tunitas/tarwater/uss/dependencies.mk
